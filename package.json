  "devDependencies": {
    "@compositor/kit": "^1.0.43",
    "@compositor/kit-snapshot": "^1.0.24",
    "@compositor/x0": "^6.0.0-4",
    "bundlesize": "^0.17.0",
    "react-docgen": "^2.21.0",
    "react-emotion": "^9.2.4",
    "react-router-dom": "^4.3.1",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1"
  },
  "x0": {
    "title": "GO1D - GO1 Design System",
